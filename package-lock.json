{
  "name": "scanner-open",
  "version": "0.1.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "scanner-open",
      "version": "0.1.0",
      "license": "MIT",
      "workspaces": [
        "packages/scanner-core",
        "packages/scanner-cli"
      ]
    },
    "node_modules/@scanner-open/cli": {
      "resolved": "packages/scanner-cli",
