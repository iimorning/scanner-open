      "resolved": "packages/scanner-core",
      "link": true
    },
    "packages/scanner-cli": {
      "name": "@scanner-open/cli",
      "version": "0.1.0",
      "license": "MIT",
      "dependencies": {
        "@scanner-open/core": "file:../scanner-core"
      },
      "bin": {
        "scanner": "bin/scanner.js"
      }
    },
    "packages/scanner-core": {
      "name": "@scanner-open/core",
      "version": "0.1.0",
      "license": "MIT"
    }
  }
}
